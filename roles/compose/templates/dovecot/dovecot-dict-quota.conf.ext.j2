connect = host=mysql_mail dbname=mailserver user={{mysql_mail_user}} password={{mysql_mail_password}}
map {
  pattern = priv/quota/storage
  table = quota2
  username_field = username
  value_field = bytes
}
map {
  pattern = priv/quota/messages
  table = quota2
  username_field = username
  value_field = messages
}
